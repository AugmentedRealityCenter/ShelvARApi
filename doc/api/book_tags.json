			"nickname":"findPetsByTags",
			"parameters":[{
				"name":"tags",
				"description":"Tags to filter by",
				"paramType":"query",
				"required":true,
				"allowMultiple":true,
				"dataType":"string"}],
			"errorResponses":[{
				"code":400,
				"reason":"Invalid tag value"}]}]}],
